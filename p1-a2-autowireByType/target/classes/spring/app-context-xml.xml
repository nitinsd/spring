<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
    	http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="categoryBean" class="he.cscie57.spring5.autowireByType.domain.model.Category" autowire="byType">
        <property name="id" value="1"/>
        <property name="name" value="Poetry"/>
    </bean>

    <bean id="bookFrost" class="he.cscie57.spring5.autowireByType.domain.model.Book">
    	<property name="id" value="14"/>
    	<property name="isbn" value="214745678"/>
    	<property name="title" value="Frost Poetry"/>
    	<property name="price" value="22.19"/>
    </bean>
    
</beans>
